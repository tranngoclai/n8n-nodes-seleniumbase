{
    "name": "n8n-nodes-seleniumbase",
    "version": "1.0.1",
    "description": "n8n community node for executing Python scraping scripts using SeleniumBase API",
    "license": "MIT",
    "homepage": "https://github.com/tranngoclai/seleniumbase",
    "keywords": [
        "n8n-community-node-package",
        "seleniumbase",
        "web-scraping",
        "automation"
    ],
    "author": {
        "name": "Tran Light",
        "email": "tranlight98@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tranngoclai/seleniumbase.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": false,
        "credentials": [
            "dist/credentials/SeleniumBaseApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/SeleniumBase/SeleniumBase.node.js"
        ]
    },
    "dependencies": {
        "form-data": "^4.0.1"
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/node": "^25.0.10",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
